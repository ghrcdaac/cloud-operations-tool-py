#!/usr/bash
export AWS_PROFILE=[REDACTED]                           # The name of the local AWS profile in ~/.aws/credentials which stores the access id and secret values for the desired environment.
export INVOKE_BASE_URL=[REDACTED]                       # The Cumulus Archive API URL value or Cumlus API Cloudfront URL where applicable
export S3URI_LAUNCHPAD_CERT=[REDACTED]                  # The AWS S3 location of the environment's Cumulus launchpad certificate
export LAUNCHPAD_PASSPHRASE_SECRET_NAME=[REDACTED]      # The AWS Secret name which stores the environment's Cumulus launchpad passphrase
export LAUNCHPAD_URL=[REDACTED]                         # Launchpad authentication endpoint (likely https://api.launchpad.nasa.gov/icam/api/sm/v1/gettoken )

# If your certificate and passphrase are not in AWS
export LAUNCHPAD_PASSPHRASE=[REDACTED]                  # Launchpad passphrase string
export FS_LAUNCHPAD_CERT=[REDACTED]                     # Local path to Launchpad certificate
